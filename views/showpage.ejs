<%- include('header')%>

<div class="container">
  <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">id</th>
            <th scope="col">userName</th>
            <th scope="col">userMobile</th>
            <th scope="col">userEmail Id</th>
            <th scope="col"> Delete</th>
          </tr>
        </thead>
        <tbody>
            <% smita.forEach(function(obj){%>
                <tr>
                    <th scope="row">1</th>
                    <td><%=obj.username%></td>
                    <td><%=obj.usermobile%></td>
                    <td><%=obj.useremail%></td>
                    <td><a href="/delete/<%=obj._id%>"><i class="far fa-trash-alt"></i></a></td>


                </tr>
                <%})%>
         </tbody>
      </table>
</div>
<%- include('footer')%>
